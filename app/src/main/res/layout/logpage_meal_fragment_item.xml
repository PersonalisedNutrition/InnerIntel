<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/logpage_item_date"
        android:layout_width="90dp"
        android:layout_height="33dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="20dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/logpage_item_time"
        android:layout_width="71dp"
        android:layout_height="29dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="20dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/logpage_item_date"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/logpage_item_type"
        android:layout_width="101dp"
        android:layout_height="34dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="20dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/logpage_item_time"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>